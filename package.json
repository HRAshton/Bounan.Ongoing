{
  "name": "bounan-animan",
  "private": true,
  "scripts": {
    "typecheck": "pnpm -r run typecheck",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "knip": "knip",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "dependencies:check": "pnpm outdated --recursive --use-stderr",
    "dependencies:update": "pnpm update --recursive --latest",
    "prerelease": "npm-run-all --parallel typecheck lint test knip dependencies:check && echo 'All checks passed!'"
  },
  "license": "BSD-3-Clause",
  "keywords": [],
  "author": {
    "name": "HRAshton"
  },
  "engines": {
    "node": ">=14.16.0",
    "pnpm": ">=10.28.1",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "packageManager": "pnpm@10.28.1",
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "knip": "catalog:",
    "npm-run-all": "catalog:",
    "typescript-eslint": "catalog:",
    "vitest": "catalog:"
  }
}
